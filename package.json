{
  "name": "app-template",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "lint": "pnpm -r exec eslint .",
    "format": "pnpm -r exec prettier . --write",
    "typecheck": "pnpm -r exec tsc -b",
    "dev-docker": "docker compose -f docker-compose.dev.yml up --build",
    "dev-docker:detached": "docker compose -f docker-compose.dev.yml up -d --build",
    "down-docker": "docker compose -f docker-compose.dev.yml down",
    "dev": "pnpm --filter web dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
